@model IEnumerable<TransporteParticular.Models.ViagemRotaTable>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Latidute)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Longitude)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdViagemNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Latidute)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Longitude)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdViagemNavigation.EnderecoChegada)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdViagemRota">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdViagemRota">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdViagemRota">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
